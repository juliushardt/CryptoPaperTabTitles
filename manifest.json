{
    "manifest_version": 3,
    "name": "Crypto Paper Tab Titles",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": ["https://eprint.iacr.org/*.pdf"],
            "include_globs": ["https://eprint.iacr.org/????/*.pdf*"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    "permissions": ["storage"],
    "host_permissions": [
        "https://eprint.iacr.org/*.pdf"
    ]
}